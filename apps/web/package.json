{
  "name": "@tabulous/web",
  "version": "0.0.1",
  "description": "Tabulous web application",
  "type": "module",
  "scripts": {
    "atelier": "vite build --mode export-atelier",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite dev --open https://localhost:3000",
    "test": "NODE_OPTIONS=--max-old-space-size=7000 vitest run --coverage",
