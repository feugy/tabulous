{
  "name": "@tabulous/web",
  "version": "0.0.1",
  "description": "Tabulous web application",
  "type": "module",
  "scripts": {
    "atelier": "NODE_OPTIONS=--max-old-space-size=4096 vite build --mode export-atelier",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "prepare": "node download-decoder && svelte-kit sync",
    "preview": "vite preview",
    "start": "vite dev --open https://localhost:3000",
    "test": "NODE_OPTIONS=--max-old-space-size=7000 vitest run --coverage",
    "test:dev": "vitest dev --reporter=verbose",
    "test:integration": "pnpm test:integration:prepare && pnpm test:integration:run",
    "test:integration:prepare": "NODE_OPTIONS=--max-old-space-size=4096 vite build --mode integration",
    "test:integration:show": "playwright show-report coverage/integration",
    "test:integration:run": "playwright test",
    "typecheck": "svelte-kit sync && svelte-check --tsconfig jsconfig.json --compiler-warnings \"a11y-autofocus:ignore,a11y-no-noninteractive-element-interactions:ignore,a11y-no-static-element-interactions:ignore\""
  },
  "devDependencies": {
    "@atelier-wb/svelte": "^0.11.0",
    "@atelier-wb/toolshot": "^0.11.0",
    "@atelier-wb/vite-plugin-atelier": "^0.11.1",
    "@babylonjs/core": "^6.21.0",
    "@babylonjs/gui": "^6.21.0",
    "@babylonjs/gui-editor": "^6.21.0",
    "@babylonjs/inspector": "^6.21.0",
    "@babylonjs/loaders": "^6.21.0",
    "@babylonjs/materials": "^6.21.0",
    "@babylonjs/serializers": "^6.21.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/static": "^6.11.1",
    "@fastify/websocket": "^8.2.0",
    "@fontsource/carter-one": "^5.0.8",
    "@fontsource/lexend": "^5.0.12",
    "@playwright/test": "^1.38.0",
    "@poppanator/sveltekit-svg": "^4.1.3",
    "@rollup/plugin-graphql": "^2.0.3",
    "@rollup/plugin-yaml": "^4.1.1",
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.25.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/svelte": "^4.0.3",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@urql/core": "^4.1.2",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "accept-language-parser": "^1.5.0",
    "autoprefixer": "^10.4.15",
    "babylonjs-gltf2interface": "^6.21.0",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "chroma-js": "^2.4.2",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "earcut": "^2.2.4",
    "fastify": "^4.23.2",
    "format-message": "^6.2.4",
    "graphql": "^16.8.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.14.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-lib-coverage": "^3.2.0",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-reports": "^3.1.6",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "material-design-icons-iconfont": "^6.7.0",
    "ms": "2.1.3",
    "msw": "^1.3.1",
    "postcss": "^8.4.29",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "rfdc": "^1.3.0",
    "rxjs": "^7.8.1",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-htm": "^1.2.0",
    "svelte-intl": "^1.1.4",
    "svelte-markdown": "^0.4.0",
    "svelte-portal": "^2.2.0",
    "svelte-windicss-preprocess": "^4.2.8",
    "v8-to-istanbul": "^9.1.0",
    "vite": "^4.4.9",
    "vite-plugin-windicss": "^1.9.1",
    "webrtc-adapter": "^8.2.3",
    "whatwg-fetch": "^3.6.19",
    "wonka": "^6.3.4"
  }
}
